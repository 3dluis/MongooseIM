OOVOO-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
    enterprises, Counter64, Gauge32
        FROM SNMPv2-SMI
    DisplayString
        FROM SNMPv2-TC
    MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
        FROM SNMPv2-CONF
    ;

oovooRegModule MODULE-IDENTITY
    LAST-UPDATED "201106131122Z"
    ORGANIZATION "oovoo.com"
    CONTACT-INFO "oovoo.com"
    DESCRIPTION
	"The root MIB module for oovoo.com"
    ::= {oovooMIB 3}


oovoo		OBJECT IDENTIFIER ::= {enterprises 40000}
oovooMIB	OBJECT IDENTIFIER ::= {oovoo 1}

oovooMIBConformance	OBJECT IDENTIFIER ::= {oovooMIB 1}
oovooMIBObjects		OBJECT IDENTIFIER ::= {oovooMIB 2}

